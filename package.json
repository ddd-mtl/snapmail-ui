{
  "name": "snapmail",
  "version": "0.0.3",
  "description": "the most wonderful mail app",
  "scripts": {
    "all": "concurrently --kill-others \"npm:billy\" \"npm:alex\" \"npm:camille\"",
    "alex": "concurrently --kill-others \"npm:alex-ui\" \"npm:alex-hc\"",
    "alex-ui": "concurrently -k --names tsc,dev-server \"web-dev-server --config web-dev-server.config.mjs\"",
    "alex-hc": "RUST_LOG=warn holochain-run-dna -p 8880 ../snapmail-rsm/snapmail.dna.gz",
    "billy": "concurrently --kill-others \"npm:billy-ui\" \"npm:billy-hc\"",
    "billy-ui": "concurrently -k --names tsc,dev-server \"web-dev-server --config web-dev-server-billy.config.mjs\"",
    "billy-hc": "RUST_LOG=warn holochain-run-dna -p 8881 ../snapmail-rsm/snapmail.dna.gz",
    "camille": "concurrently --kill-others \"npm:camille-ui\" \"npm:camille-hc\"",
    "camille-perm": "concurrently --kill-others \"npm:camille-ui\" \"npm:camille-hc-perm\"",
    "camille-ui": "concurrently -k --names tsc,dev-server \"web-dev-server --config web-dev-server-camille.config.mjs\"",
    "camille-hc": "RUST_LOG=warn holochain-run-dna -p 8882 ../snapmail-rsm/snapmail.dna.gz",
    "camille-hc-perm": "RUST_LOG=warn holochain-run-dna -p 8882 -r ./tmp/camille ../snapmail-rsm/snapmail.dna.gz",
    "build-watch": "rollup -c rollup.config.js -w",
    "build-rollup": "rimraf dist && rollup -c rollup.config.js",
    "start-old": "webpack-dev-server --open --config webpack.dev.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "esbuild": "esbuild --sourcemap --bundle src/index.js --outdir=dist",
    "esprod": "esbuild --minify --bundle src/index.js --outdir=dist",
    "devserver": "webpack-dev-server --env.mode development",
    "build": "webpack --env.mode production",
    "dev": "webpack --config webpack.dev.js",
    "release": "webpack --config webpack.release.js",
    "prod": "webpack --config webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddd-mtl/snapmail.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ddd-mtl/snapmail/issues"
  },
  "homepage": "https://github.com/ddd-mtl/snapmail#readme",
  "devDependencies": {
    "@webcomponents/webcomponentsjs": "^2.4.3",
    "copy-webpack-plugin": "^5.1.1",
    "html-webpack-plugin": "^4.2.0",
    "style-loader": "^1.0.0",
    "webpack": "^4.41.3",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@holochain/conductor-api": "0.0.1-dev.16",
    "@polymer/polymer": "^3.3.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@vaadin-component-factory/vcf-tooltip": "^1.2.2",
    "@vaadin/vaadin": "^15.0.5",
    "@vaadin/vaadin-icons": "^4.3.1",
    "@vaadin/vaadin-text-field": "^2.5.5",
    "@web/dev-server": "0.0.13",
    "byte-base64": "^1.1.0",
    "concurrently": "^5.1.0",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "prop-types": "^15.7.2",
    "rollup": "^2.32.0",
    "rollup-plugin-node-builtins": "^2.1.2",
    "sjcl": "^1.0.8"
  },
  "main": "webpack.common.js"
}
